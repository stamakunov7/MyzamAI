{
    "include": [
        "src",
        "tests",
        "scripts"
    ],
    "exclude": [
        "venv",
        "**/__pycache__",
        "**/.pytest_cache",
        "**/htmlcov",
        "**/.coverage"
    ],
    "pythonVersion": "3.9",
    "pythonPlatform": "Darwin",
    "executionEnvironments": [
        {
            "root": ".",
            "pythonVersion": "3.9",
            "pythonPlatform": "Darwin",
            "extraPaths": [
                "src",
                "tests"
            ]
        }
    ],
    "typeCheckingMode": "basic",
    "useLibraryCodeForTypes": true,
    "autoImportCompletions": true,
    "reportMissingImports": "warning",
    "reportMissingTypeStubs": false
}
